  },
  "homepage": "https://github.com/DwightHuggins/PlayDate#readme",
  "dependencies": {
    "@material-ui/styles": "^4.10.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.27",
    "react-typical": "^0.1.3"
  },
